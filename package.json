{
  "name": "@real-estate-agency/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently -n api,client,admin -c blue,green,yellow \"npm run dev:api\" \"npm run dev:client\" \"npm run dev:admin\"",
    "dev:api": "npm run dev -w @real-estate-agency/api-server",
    "dev:client": "npm run dev -w @real-estate-agency/client-portal",
    "dev:admin": "npm run dev -w @real-estate-agency/admin-panel",
    "dev:prisma": "npm run studio -w @real-estate-agency/db",
    "build": "npm run build --workspaces",
    "build:api": "npm run build -w @real-estate-agency/api-server",
    "build:client": "npm run build -w @real-estate-agency/client-portal",
    "build:admin": "npm run build -w @real-estate-agency/admin-panel",
    "start:api": "npm run start -w @real-estate-agency/api-server",
    "db:seed": "ts-node libs/shared/db/prisma/seed.ts",
    "db:studio": "npm run studio -w @real-estate-agency/db",
    "lint": "eslint .",
    "format": "prettier --write \"apps/**/src/**/*.{ts,tsx}\" \"libs/**/src/**/*.{ts,tsx}\""
  },
  "private": true,
  "workspaces": [
    "apps/*",
    "libs/shared/*"
  ],
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@playwright/test": "^1.36.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/jest": "^29.5.0",
    "@types/node": "20.19.9",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/ui": "^2.1.0",
    "autoprefixer": "^10.4.24",
    "concurrently": "^9.2.1",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jsdom": "~22.1.0",
    "postcss": "^8.5.6",
    "prettier": "~3.6.2",
    "prisma": "^6.19.2",
    "tailwindcss": "^4.1.18",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.3.0",
    "tsx": "^4.19.0",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.40.0",
    "vite": "^5.4.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.0",
    "webpack": "^5.99.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^11.0.0",
    "@prisma/client": "^6.19.2",
    "@radix-ui/react-slot": "^1.2.4",
    "@tanstack/react-query": "^5.90.20",
    "@types/leaflet": "^1.9.21",
    "axios": "^1.6.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.563.0",
    "next": "^14.2.15",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.71.1",
    "react-leaflet": "^5.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.3.6"
  }
}
